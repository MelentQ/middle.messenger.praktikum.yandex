<div class="chat">
  <div class="chat__side">
    <div class="chat__menu">
      {{{iconLink}}}
    </div>
    {{{searchForm}}}
    <ul class="chat__list">
      {{#each cards }}
        <li class="chat__list-item">
          {{{this}}}
        </li>
      {{/each}}
    </ul>
  </div>
  <div class="chat__content">
    {{{chatContent}}}
  </div>
</div>
